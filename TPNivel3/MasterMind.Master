                        </li>
                    </ul>
                </div>
                <div class="d-flex">

                    <% if (!negocio.Seguridad.sesionActiva(Session["usuario"])) { %>
                    <a href="Login.aspx" class="btn btn-success me-2">Login</a>
                    <a href="Registro.aspx" class="btn btn-success m-auto">Registrarse</a>
                    <%
                        }
                        else
                        {
                    %>
                    <asp:Button Text="Salir" ID="btnSalir" CssClass="btn btn-success me-2" OnClick="btnSalir_Click" runat="server" />
                    <asp:Label Text="User" runat="server" ID="lblUser" CssClass="text-light m-auto me-2" />
                    <% } %>
                    <asp:Image runat="server" ID="imgAvatar" Style="height: 40px; border-radius: 50%; width:40px; margin-left: 7px;" />
                </div>
            </div>
        </nav>
         
        <div class=" mt-md-5 m-xxl-5">
            <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
            </asp:ContentPlaceHolder>
